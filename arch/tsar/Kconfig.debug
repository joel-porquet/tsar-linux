menu "Kernel hacking"

config TRACE_IRQFLAGS_SUPPORT
	bool
	default y

source "lib/Kconfig.debug"

config EARLY_PRINTK
	bool "Early printk" if EXPERT
	default y
	help
	  This option enables special console drivers which allow the kernel
	  to print messages very early in the bootup process.

config MEMBLOCK_DEBUG
	bool "Memblock API debug" if EXPERT
	default n
	help
	  This option enables the debugging facility of the memblock API.

endmenu
